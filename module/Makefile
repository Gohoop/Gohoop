all: gbase gdata gdfs gmapreduce

clean:
	$(MAKE) -C GBase clean
	$(MAKE) -C GData clean
	$(MAKE) -C GDFS clean
	$(MAKE) -C GMapReduce clean

cleanall:
	$(MAKE) -C GBase cleanall
	$(MAKE) -C GData cleanall
	$(MAKE) -C GDFS cleanall
	$(MAKE) -C GMapReduce cleanall

gbase:
	$(MAKE) -C GBase 

gdata:
	$(MAKE) -C GData

gdfs:    
	$(MAKE) -C GDFS    

gmapreduce:    
	$(MAKE) -C GMapReduce